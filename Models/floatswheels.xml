<?xml version="1.0"?>

<PropertyList>

  <path>floatswheels.ac</path>

  <animation>
    <!-- Objets opaques -->
    <object-name>flotteurs</object-name>
    <object-name>Ldamper</object-name>
    <object-name>Lstrut1</object-name>
    <object-name>Lstrut2</object-name>
    <object-name>Rdamper</object-name>
    <object-name>Rstrut1</object-name>
    <object-name>Rstrut2</object-name>
    <object-name>roueFG</object-name>
    <object-name>roueFD</object-name>
    <object-name>roueAG</object-name>
    <object-name>roueAD</object-name>
    <object-name>axeFG</object-name>
    <object-name>axeFD</object-name>

    <!-- Objets transparents -->
  </animation>

  <animation>
    <type>rotate</type>
    <object-name>Ldamper</object-name>
    <object-name>roueAG</object-name>
    <property>gear/gear[4]/compression-m</property>
    <factor>400</factor>
    <center>
      <x-m>-5.527</x-m>
      <y-m>-1.640</y-m>
      <z-m>-2.209</z-m>
    </center>
    <axis>
      <x>0</x>
      <y>-1</y>
      <z>0</z>
    </axis>
  </animation>

  <animation>
    <type>rotate</type>
    <object-name>Rdamper</object-name>
    <object-name>roueAD</object-name>
    <property>gear/gear[5]/compression-m</property>
    <factor>400</factor>
    <center>
      <x-m>-5.527</x-m>
      <y-m>1.640</y-m>
      <z-m>-2.209</z-m>
    </center>
    <axis>
      <x>0</x>
      <y>-1</y>
      <z>0</z>
    </axis>
  </animation>

  <animation>
    <type>translate</type>
    <object-name>roueFG</object-name>
    <object-name>axeFG</object-name>
    <property>gear/gear[6]/position-norm</property>
    <interpolation>
      <entry><ind>0.0</ind><dep>-0.34</dep></entry>
      <entry><ind>1.0</ind><dep>0.0</dep></entry>
    </interpolation>
    <center>
      <x-m>-2.115</x-m>
      <y-m>1.640</y-m>
      <z-m>-2.335</z-m>
    </center>
    <axis>
      <x>0</x>
      <y>0</y>
      <z>-1.0</z>
    </axis>
  </animation>

  <animation>
    <type>translate</type>
    <object-name>roueFG</object-name>
    <property>gear/gear[6]/compression-m</property>
    <factor>1.0</factor>
    <center>
      <x-m>-2.115</x-m>
      <y-m>1.640</y-m>
      <z-m>-2.335</z-m>
    </center>
    <axis>
      <x>0</x>
      <y>0</y>
      <z>1.0</z>
    </axis>
  </animation>


  <animation>
    <type>translate</type>
    <object-name>roueFD</object-name>
    <object-name>axeFD</object-name>
    <property>gear/gear[7]/position-norm</property>
    <interpolation>
      <entry><ind>0.0</ind><dep>-0.34</dep></entry>
      <entry><ind>1.0</ind><dep>0.0</dep></entry>
    </interpolation>
    <center>
      <x-m>-2.115</x-m>
      <y-m>-1.640</y-m>
      <z-m>-2.335</z-m>
    </center>
    <axis>
      <x>0</x>
      <y>0</y>
      <z>-1.0</z>
    </axis>
  </animation>

  <animation>
    <type>translate</type>
    <object-name>roueFD</object-name>
    <property>gear/gear[7]/compression-m</property>
    <factor>1.0</factor>
    <center>
      <x-m>-2.115</x-m>
      <y-m>-1.640</y-m>
      <z-m>-2.335</z-m>
    </center>
    <axis>
      <x>0</x>
      <y>0</y>
      <z>1.0</z>
    </axis>
  </animation>

  <animation>
    <type>spin</type>
    <object-name>roueFG</object-name>
    <property>gear/gear[4]/rollspeed-ms</property>
    <factor> 30 </factor>
    <center>
      <x-m> -2.045 </x-m>
      <y-m> -1.640 </y-m>
      <z-m> -2.331 </z-m>
    </center>
    <axis>
      <x>  0.000 </x>
      <y> -1.000 </y>
      <z>  0.000 </z>
    </axis>
  </animation>

  <animation>
    <type>spin</type>
    <object-name>roueFD</object-name>
    <property>gear/gear[5]/rollspeed-ms</property>
    <factor> 30 </factor>
    <center>
      <x-m> -2.045 </x-m>
      <y-m>  1.640 </y-m>
      <z-m> -2.331 </z-m>
    </center>
    <axis>
      <x>  0.000 </x>
      <y> -1.000 </y>
      <z>  0.000 </z>
    </axis>
  </animation>

  <animation>
    <type>spin</type>
    <object-name>roueAG</object-name>
    <property>gear/gear[6]/rollspeed-ms</property>
    <factor> 30 </factor>
    <center>
      <x-m> -6.314 </x-m>
      <y-m> -1.640 </y-m>
      <z-m> -2.294 </z-m>
    </center>
    <axis>
      <x>  0.000 </x>
      <y> -1.000 </y>
      <z>  0.000 </z>
    </axis>
  </animation>

  <animation>
    <type>spin</type>
    <object-name>roueAD</object-name>
    <property>gear/gear[7]/rollspeed-ms</property>
    <factor> 30 </factor>
    <center>
      <x-m> -6.314 </x-m>
      <y-m>  1.640 </y-m>
      <z-m> -2.294 </z-m>
    </center>
    <axis>
      <x>  0.000 </x>
      <y> -1.000 </y>
      <z>  0.000 </z>
    </axis>
  </animation>

</PropertyList>
