<?xml version="1.0" encoding="UTF-8"?>

<PropertyList>

  <path>floatswheels.ac</path>

  <animation>
    <!-- Objets opaques -->
    <object-name>flotteurs</object-name>
    <object-name>Ldamper</object-name>
    <object-name>Lstrut1</object-name>
    <object-name>Lstrut2</object-name>
    <object-name>Rdamper</object-name>
    <object-name>Rstrut1</object-name>
    <object-name>Rstrut2</object-name>
    <object-name>roueFG</object-name>
    <object-name>roueFD</object-name>
    <object-name>roueAG</object-name>
    <object-name>roueAD</object-name>
    <object-name>axeFG</object-name>
    <object-name>axeFD</object-name>

    <!-- Objets transparents -->
  </animation>

  <animation>
    <type>material</type>
    <object-name>flotteurs</object-name>
    <object-name>Ldamper</object-name>
    <object-name>Lstrut1</object-name>
    <object-name>Lstrut2</object-name>
    <object-name>Rdamper</object-name>
    <object-name>Rstrut1</object-name>
    <object-name>Rstrut2</object-name>
    <object-name>roueFG</object-name>
    <object-name>roueFD</object-name>
    <object-name>roueAG</object-name>
    <object-name>roueAD</object-name>
    <object-name>axeFG</object-name>
    <object-name>axeFD</object-name>

    <property-base>sim/model/livery</property-base>
    <texture-prop>texture</texture-prop>
    <texture>texture.png</texture>
  </animation>

  <!-- Trainée gauche -->
  <model>
    <path>Aircraft/dhc3/Models/Effects/wakeGA.xml</path>
    <offsets>
      <x-m>  0.992 </x-m>
      <y-m> -1.640 </y-m>
      <z-m> -1.594 </z-m>
      <roll-deg>    0.000 </roll-deg>
      <pitch-deg> -45.000 </pitch-deg>
      <heading-deg> 0.000 </heading-deg>
    </offsets>
  </model> 

  <!-- Trainée droite -->
  <model>
    <path>Aircraft/dhc3/Models/Effects/wakeDA.xml</path>
    <offsets>
      <x-m>  0.992 </x-m>
      <y-m>  1.640 </y-m>
      <z-m> -1.594 </z-m>
      <roll-deg>    0.000 </roll-deg>
      <pitch-deg> -45.000 </pitch-deg>
      <heading-deg> 0.000 </heading-deg>
    </offsets>
  </model> 

  <!-- Eclaboussures, avant gauche -->
  <model>
    <path>Aircraft/dhc3/Models/Effects/jetGA.xml</path>
    <offsets>
      <x-m> -6.223 </x-m>
      <y-m> -1.640 </y-m>
      <z-m> -1.852 </z-m>
      <roll-deg>    0.000 </roll-deg>
      <pitch-deg> -90.000 </pitch-deg>
      <heading-deg> 0.000 </heading-deg>
    </offsets>
  </model> 

  <!-- Eclaboussures, avant droite -->
  <model>
    <path>Aircraft/dhc3/Models/Effects/jetDA.xml</path>
    <offsets>
      <x-m> -6.223 </x-m>
      <y-m>  1.640 </y-m>
      <z-m> -1.852 </z-m>
      <roll-deg>    0.000 </roll-deg>
      <pitch-deg> -90.000 </pitch-deg>
      <heading-deg> 0.000 </heading-deg>
    </offsets>
  </model> 

  <!-- Eclaboussures, centre gauche -->
  <model>
    <path>Aircraft/dhc3/Models/Effects/jetmGA.xml</path>
    <offsets>
      <x-m> -2.635 </x-m>
      <y-m> -1.640 </y-m>
      <z-m> -1.896 </z-m>
      <roll-deg>    0.000 </roll-deg>
      <pitch-deg> -90.000 </pitch-deg>
      <heading-deg> 0.000 </heading-deg>
    </offsets>
  </model> 

  <!-- Eclaboussures, centre droite -->
  <model>
    <path>Aircraft/dhc3/Models/Effects/jetmDA.xml</path>
    <offsets>
      <x-m> -2.635 </x-m>
      <y-m>  1.640 </y-m>
      <z-m> -1.896 </z-m>
      <roll-deg>    0.000 </roll-deg>
      <pitch-deg> -90.000 </pitch-deg>
      <heading-deg> 0.000 </heading-deg>
    </offsets>
  </model>

  <animation>
    <name>EnsembleAvantG</name>
    <object-name>Ldamper</object-name>
    <object-name>Lstrut2</object-name>
    <object-name>roueAG</object-name>
  </animation>

  <animation>
    <name>EnsembleAvantD</name>
    <object-name>Rdamper</object-name>
    <object-name>Rstrut2</object-name>
    <object-name>roueAD</object-name>
  </animation>

  <animation>
    <name>RoueAvantG</name>
    <object-name>Ldamper</object-name>
    <object-name>roueAG</object-name>
  </animation>

  <animation>
    <name>RoueAvantD</name>
    <object-name>Rdamper</object-name>
    <object-name>roueAD</object-name>
  </animation>

  <animation>
    <type>translate</type>
    <object-name>EnsembleAvantG</object-name>
    <property>gear/gear[0]/position-norm</property>
    <interpolation>
      <entry>
        <ind>  0.0 </ind>
        <dep> -0.28 </dep>
      </entry>
      <entry>
        <ind> 1.0 </ind>
        <dep> 0.0 </dep>
      </entry>
    </interpolation>
    <center>
      <x-m> -6.375 </x-m>
      <y-m> -1.640 </y-m>
      <z-m> -1.880 </z-m>
    </center>
    <axis>
      <x> -0.042258 </x>
      <y>  0.000000 </y>
      <z> -0.099868 </z>
    </axis>
  </animation>

  <animation>
    <type>translate</type>
    <object-name>EnsembleAvantD</object-name>
    <property>gear/gear[1]/position-norm</property>
    <interpolation>
      <entry>
        <ind>  0.00 </ind>
        <dep> -0.28 </dep>
      </entry>
      <entry>
        <ind> 1.0 </ind>
        <dep> 0.0 </dep>
      </entry>
    </interpolation>
    <center>
      <x-m> -6.375 </x-m>
      <y-m>  1.640 </y-m>
      <z-m> -1.880 </z-m>
    </center>
    <axis>
      <x> -0.042258 </x>
      <y>  0.000000 </y>
      <z> -0.099868 </z>
    </axis>
  </animation>

  <animation>
    <type>rotate</type>
    <object-name>RoueAvantG</object-name>
    <property>gear/gear[0]/compression-m</property>
    <interpolation>
      <entry>
        <ind> 0.00 </ind>
        <dep> 0.00 </dep>
      </entry>
      <entry>
        <ind>   1.0 </ind>
        <dep> -50.0 </dep>
      </entry>
    </interpolation>
    <center>
      <x-m> -6.395 </x-m>
      <y-m> -1.640 </y-m>
      <z-m> -2.054 </z-m>
    </center>
    <axis>
      <x> 0 </x>
      <y> 1 </y>
      <z> 0 </z>
    </axis>
  </animation>

  <animation>
    <type>rotate</type>
    <object-name>RoueAvantD</object-name>
    <property>gear/gear[1]/compression-m</property>
    <interpolation>
      <entry>
        <ind> 0.00 </ind>
        <dep> 0.00 </dep>
      </entry>
      <entry>
        <ind>   1.0 </ind>
        <dep> -50.0 </dep>
      </entry>
    </interpolation>
    <center>
      <x-m> -6.395 </x-m>
      <y-m>  1.640 </y-m>
      <z-m> -2.054 </z-m>
    </center>
    <axis>
      <x>0</x>
      <y>1</y>
      <z>0</z>
    </axis>
  </animation>

  <animation>
    <type>translate</type>
    <object-name>roueFG</object-name>
    <object-name>axeFG</object-name>
    <property>gear/gear[2]/position-norm</property>
    <interpolation>
      <entry>
        <ind>0.0</ind>
        <dep>-0.34</dep>
      </entry>
      <entry>
        <ind>1.0</ind>
        <dep>0.0</dep>
      </entry>
    </interpolation>
    <center>
      <x-m>-2.115</x-m>
      <y-m>1.640</y-m>
      <z-m>-2.335</z-m>
    </center>
    <axis>
      <x>0</x>
      <y>0</y>
      <z>-1.0</z>
    </axis>
  </animation>

  <animation>
    <type>translate</type>
    <object-name>roueFG</object-name>
    <property>gear/gear[2]/compression-m</property>
    <factor>1.0</factor>
    <center>
      <x-m>-2.115</x-m>
      <y-m>1.640</y-m>
      <z-m>-2.335</z-m>
    </center>
    <axis>
      <x>0</x>
      <y>0</y>
      <z>1.0</z>
    </axis>
  </animation>


  <animation>
    <type>translate</type>
    <object-name>roueFD</object-name>
    <object-name>axeFD</object-name>
    <property>gear/gear[3]/position-norm</property>
    <interpolation>
      <entry><ind>0.0</ind><dep>-0.34</dep></entry>
      <entry><ind>1.0</ind><dep>0.0</dep></entry>
    </interpolation>
    <center>
      <x-m>-2.115</x-m>
      <y-m>-1.640</y-m>
      <z-m>-2.335</z-m>
    </center>
    <axis>
      <x>0</x>
      <y>0</y>
      <z>-1.0</z>
    </axis>
  </animation>

  <animation>
    <type>translate</type>
    <object-name>roueFD</object-name>
    <property>gear/gear[3]/compression-m</property>
    <factor>1.0</factor>
    <center>
      <x-m>-2.115</x-m>
      <y-m>-1.640</y-m>
      <z-m>-2.335</z-m>
    </center>
    <axis>
      <x>0</x>
      <y>0</y>
      <z>1.0</z>
    </axis>
  </animation>

  <animation>
    <type>spin</type>
    <object-name>roueAG</object-name>
    <property>gear/gear[0]/rollspeed-ms</property>
    <factor> 30 </factor>
    <center>
      <x-m> -6.314 </x-m>
      <y-m> -1.640 </y-m>
      <z-m> -2.294 </z-m>
    </center>
    <axis>
      <x>  0.000 </x>
      <y> -1.000 </y>
      <z>  0.000 </z>
    </axis>
  </animation>

  <animation>
    <type>spin</type>
    <object-name>roueAD</object-name>
    <property>gear/gear[1]/rollspeed-ms</property>
    <factor> 30 </factor>
    <center>
      <x-m> -6.314 </x-m>
      <y-m>  1.640 </y-m>
      <z-m> -2.294 </z-m>
    </center>
    <axis>
      <x>  0.000 </x>
      <y> -1.000 </y>
      <z>  0.000 </z>
    </axis>
  </animation>

  <animation>
    <type>spin</type>
    <object-name>roueFG</object-name>
    <property>gear/gear[2]/rollspeed-ms</property>
    <factor> 30 </factor>
    <center>
      <x-m> -2.045 </x-m>
      <y-m> -1.640 </y-m>
      <z-m> -2.331 </z-m>
    </center>
    <axis>
      <x>  0.000 </x>
      <y> -1.000 </y>
      <z>  0.000 </z>
    </axis>
  </animation>

  <animation>
    <type>spin</type>
    <object-name>roueFD</object-name>
    <property>gear/gear[3]/rollspeed-ms</property>
    <factor> 30 </factor>
    <center>
      <x-m> -2.045 </x-m>
      <y-m>  1.640 </y-m>
      <z-m> -2.331 </z-m>
    </center>
    <axis>
      <x>  0.000 </x>
      <y> -1.000 </y>
      <z>  0.000 </z>
    </axis>
  </animation>

</PropertyList>

