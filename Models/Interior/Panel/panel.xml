<?xml version="1.0"?>

<PropertyList>

  <path>panel.ac</path>

  <animation>
    <!-- Objets opaques -->
    <object-name>panel</object-name>
    <object-name>panel1</object-name>
    <object-name>panel2</object-name>
    <object-name>manetteG</object-name>
    <object-name>manetteD</object-name>
    <object-name>manette1</object-name>
    <object-name>manette2</object-name>
    <object-name>manette3</object-name>
    <object-name>yoke</object-name>
    <object-name>yoke1</object-name>
    <object-name>yoke2</object-name>
    <object-name>LHpedal</object-name>
    <object-name>RHpedal</object-name>
    <object-name>throttle</object-name>
    <object-name>propeller</object-name>
    <object-name>mixture</object-name>

    <!-- Objets transparents -->
  </animation>

  <!-- GPS -->
  <model>
    <path>Aircraft/Instruments-3d/zkv500/ZKV500.xml</path>
    <offsets>
      <x-m>  0.090 </x-m>
      <y-m> -0.169 </y-m>
      <z-m>  0.457 </z-m>
    </offsets>
  </model>

  <!-- asi -->
  <model>
    <path>Aircraft/dhc3/Models/Interior/Panel/Instruments/asi/asi.xml</path>
    <offsets>
      <x-m>  0.089 </x-m>
      <y-m> -0.414 </y-m>
      <z-m>  0.353 </z-m>
    </offsets>
  </model>

  <!-- ai -->
  <model>
    <path>Aircraft/dhc3/Models/Interior/Panel/Instruments/ai/ai.xml</path>
    <offsets>
      <x-m>  0.089 </x-m>
      <y-m> -0.299 </y-m>
      <z-m>  0.353 </z-m>
    </offsets>
  </model>

  <!-- alt -->
  <model>
    <path>Aircraft/dhc3/Models/Interior/Panel/Instruments/alt/alt.xml</path>
    <offsets>
      <x-m>  0.089 </x-m>
      <y-m> -0.184 </y-m>
      <z-m>  0.353 </z-m>
    </offsets>
  </model>

  <!-- turn -->
  <model>
    <path>Aircraft/dhc3/Models/Interior/Panel/Instruments/turn/turn.xml</path>
    <offsets>
      <x-m>  0.089 </x-m>
      <y-m> -0.414 </y-m>
      <z-m>  0.246 </z-m>
    </offsets>
  </model>

  <!-- hdg -->
  <model>
    <path>Aircraft/dhc3/Models/Interior/Panel/Instruments/hdg/hdg.xml</path>
    <offsets>
      <x-m>  0.089 </x-m>
      <y-m> -0.299 </y-m>
      <z-m>  0.246 </z-m>
    </offsets>
  </model>

  <!-- vsi -->
  <model>
    <path>Aircraft/dhc3/Models/Interior/Panel/Instruments/vsi/vsi.xml</path>
    <offsets>
      <x-m>  0.089 </x-m>
      <y-m> -0.184 </y-m>
      <z-m>  0.246 </z-m>
    </offsets>
  </model>

  <!-- clock -->
  <model>
    <path>Aircraft/dhc3/Models/Interior/Panel/Instruments/clock/clock.xml</path>
    <offsets>
      <x-m>  0.089 </x-m>
      <y-m> -0.414 </y-m>
      <z-m>  0.143 </z-m>
    </offsets>
  </model>

  <!-- adf -->
  <model>
    <path>Aircraft/dhc3/Models/Interior/Panel/Instruments/adf/adf.xml</path>
    <offsets>
      <x-m>  0.089 </x-m>
      <y-m> -0.299 </y-m>
      <z-m>  0.143 </z-m>
    </offsets>
  </model>

  <!-- ki206 -->
  <model>
    <path>Aircraft/dhc3/Models/Interior/Panel/Instruments/ki206/ki206.xml</path>
    <offsets>
      <x-m>  0.089 </x-m>
      <y-m> -0.184 </y-m>
      <z-m>  0.143 </z-m>
    </offsets>
  </model>

  <!-- Flaps gauge -->
  <model>
    <path>Aircraft/dhc3/Models/Interior/Panel/Instruments/flaps/flap-gauge.xml</path>
    <offsets>
      <x-m>  0.087 </x-m>
      <y-m> -0.506 </y-m>
      <z-m>  0.141 </z-m>
    </offsets>
  </model>

  <!-- manifold -->
  <model>
    <path>Aircraft/dhc3/Models/Interior/Panel/Instruments/engine/man-gauge.xml</path>
    <offsets>
      <x-m>  0.088 </x-m>
      <y-m> -0.070 </y-m>
      <z-m>  0.450 </z-m>
    </offsets>
  </model>

  <!-- rpm -->
  <model>
    <path>Aircraft/dhc3/Models/Interior/Panel/Instruments/engine/rpm-gauge.xml</path>
    <offsets>
      <x-m> 0.088 </x-m>
      <y-m> 0.016 </y-m>
      <z-m> 0.450 </z-m>
    </offsets>
  </model>

  <!-- Température carburant -->
  <model>
    <path>Aircraft/dhc3/Models/Interior/Panel/Instruments/fuel/carbtemp.xml</path>
    <offsets>
      <x-m>  0.088 </x-m>
      <y-m> -0.070 </y-m>
      <z-m>  0.365 </z-m>
    </offsets>
  </model>

  <!-- Quantité carburant -->
  <model>
    <path>Aircraft/dhc3/Models/Interior/Panel/Instruments/fuel/fuel-qty.xml</path>
    <offsets>
      <x-m> 0.088 </x-m>
      <y-m> 0.016 </y-m>
      <z-m> 0.365 </z-m>
    </offsets>
  </model>

  <!-- Pression d'huile -->
  <model>
    <path>Aircraft/dhc3/Models/Interior/Panel/Instruments/oil/oilpsi.xml</path>
    <offsets>
      <x-m>  0.088 </x-m>
      <y-m> -0.084 </y-m>
      <z-m>  0.293 </z-m>
    </offsets>
  </model>

  <!-- Température d'huile -->
  <model>
    <path>Aircraft/dhc3/Models/Interior/Panel/Instruments/oil/oiltemp.xml</path>
    <offsets>
      <x-m>  0.088 </x-m>
      <y-m> -0.029 </y-m>
      <z-m>  0.293 </z-m>
    </offsets>
  </model>

  <!-- Ampère mètre -->
  <model>
    <path>Aircraft/dhc3/Models/Interior/Panel/Instruments/amps/ammeter.xml</path>
    <offsets>
      <x-m> 0.088 </x-m>
      <y-m> 0.027 </y-m>
      <z-m> 0.293 </z-m>
    </offsets>
  </model>

  <!-- Yokes and pedals based on DHC 6 by Syd ADAMS -->
  <animation>
    <type>translate</type>
    <object-name>RHpedal</object-name>
    <property>controls/flight/rudder</property>
    <factor>0.025</factor>
    <axis>
      <x>-1</x>
      <y>0</y>
      <z>0</z>
    </axis>
  </animation>

  <animation>
    <type>translate</type>
    <object-name>LHpedal</object-name>
    <property>controls/flight/rudder</property>
    <factor>0.025</factor>
    <axis>
      <x>1</x>
      <y>0</y>
      <z>0</z>
    </axis>
  </animation>

  <animation>
    <type>rotate</type>
    <object-name>Yoke</object-name>
    <object-name>Yoke1</object-name>
    <object-name>Yoke2</object-name>
    <property>controls/flight/elevator</property>
    <factor>10.0</factor>
    <center>
      <x-m>  0.409 </x-m>
      <y-m>  0.000 </y-m>
      <z-m> -0.445 </z-m>
    </center>
    <axis>
      <x>0</x>
      <y>-1</y>
      <z>0</z>
    </axis>
  </animation>

  <animation>
    <type>rotate</type>
    <object-name>Yoke1</object-name>
    <property>controls/flight/aileron</property>
    <factor>40.0</factor>
    <center>
      <x-m>  0.450 </x-m>
      <y-m> -0.384 </y-m>
      <z-m>  0.204 </z-m>
    </center>
    <axis>
      <x>-1</x>
      <y>0</y>
      <z>0</z>
    </axis>
  </animation>

  <animation>
    <type>rotate</type>
    <object-name>Yoke2</object-name>
    <property>controls/flight/aileron</property>
    <factor>40.0</factor>
    <center>
      <x-m> 0.411 </x-m>
      <y-m> 0.384 </y-m>
      <z-m> 0.204 </z-m>
    </center>
    <axis>
      <x>-1</x>
      <y>0</y>
      <z>0</z>
    </axis>
   </animation>

  <animation>
    <type>rotate</type>
    <object-name>throttle</object-name>
    <property>controls/engines/engine/throttle</property>
    <factor>85</factor>
    <center>
      <x-m>  0.100 </x-m>
      <y-m> -0.059 </y-m>
      <z-m>  0.162 </z-m>
    </center>
    <axis>
      <x>  0 </x>
      <y> -1 </y>
      <z>  0 </z>
    </axis>
  </animation>

  <animation>
    <type>rotate</type>
    <object-name>propeller</object-name>
    <property>controls/engines/engine/propeller-pitch</property>
    <factor>85</factor>
    <center>
      <x-m> 0.100 </x-m>
      <y-m> 0.000 </y-m>
      <z-m> 0.162 </z-m>
    </center>
    <axis>
      <x>  0 </x>
      <y> -1 </y>
      <z>  0 </z>
    </axis>
  </animation>

  <animation>
    <type>rotate</type>
    <object-name>mixture</object-name>
    <property>controls/engines/engine/mixture</property>
    <factor>85</factor>
    <center>
      <x-m> 0.100 </x-m>
      <y-m> 0.059 </y-m>
      <z-m> 0.162 </z-m>
    </center>
    <axis>
      <x>  0 </x>
      <y> -1 </y>
      <z>  0 </z>
    </axis>
  </animation>

</PropertyList>

