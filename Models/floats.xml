<?xml version="1.0"?>

<PropertyList>

  <path>floats.ac</path>

  <animation>
    <!-- Objets opaques -->
    <object-name>flotteurs0</object-name>

    <!-- Objets transparents -->
  </animation>

  <!-- Trainée gauche -->
  <model>
    <path>Aircraft/dhc3/Models/Effects/wakeG.xml</path>
    <offsets>
      <x-m>  0.992 </x-m>
      <y-m> -1.640 </y-m>
      <z-m> -1.594 </z-m>
      <roll-deg>    0.000 </roll-deg>
      <pitch-deg> -45.000 </pitch-deg>
      <heading-deg> 0.000 </heading-deg>
    </offsets>
  </model> 

  <!-- Trainée droite -->
  <model>
    <path>Aircraft/dhc3/Models/Effects/wakeD.xml</path>
    <offsets>
      <x-m>  0.992 </x-m>
      <y-m>  1.640 </y-m>
      <z-m> -1.594 </z-m>
      <roll-deg>    0.000 </roll-deg>
      <pitch-deg> -45.000 </pitch-deg>
      <heading-deg> 0.000 </heading-deg>
    </offsets>
  </model> 

  <!-- Eclaboussures, avant gauche -->
  <model>
    <path>Aircraft/dhc3/Models/Effects/jetG.xml</path>
    <offsets>
      <x-m> -6.223 </x-m>
      <y-m> -1.640 </y-m>
      <z-m> -1.852 </z-m>
      <roll-deg>    0.000 </roll-deg>
      <pitch-deg> -90.000 </pitch-deg>
      <heading-deg> 0.000 </heading-deg>
    </offsets>
  </model> 

  <!-- Eclaboussures, avant droite -->
  <model>
    <path>Aircraft/dhc3/Models/Effects/jetD.xml</path>
    <offsets>
      <x-m> -6.223 </x-m>
      <y-m>  1.640 </y-m>
      <z-m> -1.852 </z-m>
      <roll-deg>    0.000 </roll-deg>
      <pitch-deg> -90.000 </pitch-deg>
      <heading-deg> 0.000 </heading-deg>
    </offsets>
  </model> 

  <!-- Eclaboussures, centre gauche -->
  <model>
    <path>Aircraft/dhc3/Models/Effects/jetmG.xml</path>
    <offsets>
      <x-m> -2.635 </x-m>
      <y-m> -1.640 </y-m>
      <z-m> -1.896 </z-m>
      <roll-deg>    0.000 </roll-deg>
      <pitch-deg> -90.000 </pitch-deg>
      <heading-deg> 0.000 </heading-deg>
    </offsets>
  </model> 

  <!-- Eclaboussures, centre droite -->
  <model>
    <path>Aircraft/dhc3/Models/Effects/jetmD.xml</path>
    <offsets>
      <x-m> -2.635 </x-m>
      <y-m>  1.640 </y-m>
      <z-m> -1.896 </z-m>
      <roll-deg>    0.000 </roll-deg>
      <pitch-deg> -90.000 </pitch-deg>
      <heading-deg> 0.000 </heading-deg>
    </offsets>
  </model> 

</PropertyList>
