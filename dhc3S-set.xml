<?xml version="1.0" encoding="ISO-8859-1"?>

<PropertyList include="Systems/dhc3-base.xml">

  <sim>

    <description>DHC 3 Otter with skis (YASim)</description>
    <author>Emmanuel BARANGER (3D/FDM), Pierre GEOFFROY (FDM), Laurent HAYVEL (Texture)</author>
    
    <aircraft-version>0.1</aircraft-version>
    <status>0.1 Alpha</status>
    
    <flight-model>yasim</flight-model>
    <aero>Systems/FDM/dhc3S-yasim</aero>
    
    <variant-of>dhc3W</variant-of>

    <rating>
      <FDM type="int">     3 </FDM>
      <systems type="int"> 3 </systems>
      <cockpit type="int"> 3 </cockpit>
      <model type="int">   4 </model>
    </rating>

    <model>
      <path archive="y">Aircraft/dhc3/Models/dhc3S.xml</path>
      <livery>
        <file type="string">default</file>
      </livery>
    </model>

    <sound>
      <path>Aircraft/dhc3/Sounds/dhc3-sound.xml</path>
    </sound>

    <startup>
      <splash-texture>Aircraft/dhc3/Splashs/dhc3S-splash.png</splash-texture>
    </startup>

    <help include="Systems/Help/dhc3-help.xml"/>

  </sim>

  <input include="Systems/Keyboard/dhc3-keyboard.xml"/>

</PropertyList>
